	Escribir  "La cara 3 ha salido " vector[3] " con un porcentaje de " 100*vector[3]/6000
	Escribir  "La cara 4 ha salido " vector[4] " con un porcentaje de " 100*vector[4]/6000
	Escribir  "La cara 5 ha salido " vector[5] " con un porcentaje de " 100*vector[5]/6000
	Escribir  "La cara 6 ha salido " vector[6] " con un porcentaje de " 100*vector[6]/6000
	
FinAlgoritmo
