Algoritmo dados
	dimension vector[6]
	para i=1 hasta 6000 Hacer
		num=Aleatorio(1,6)
		vector[num]=vector[num]+1
	FinPara
	
	Escribir  "La cara 1 ha salido " vector[1] " con un porcentaje de " 100*vector[1]/6000
